#: ../jsm/modules/mod_privacy.c:965 ../jsm/modules/mod_privacy.c:1005
#: ../jsm/modules/mod_privacy.c:1106
msgid "The server cannot accept that privacy list name."
msgstr "Der Server kann den Namen der Privacy-Liste nicht annehmen."

#: ../jsm/modules/mod_privacy.c:1137
msgid "Privacy list is invalid: item without order attribute"
msgstr "Privacy-Liste ist ungültig: Item ohne order-Attribut"

#: ../jsm/modules/mod_privacy.c:1142
msgid "Privacy list is invalid: item action has to be either 'deny' or 'allow'"
msgstr ""
"Privacy-Liste ist ungültig: Item action muss entweder 'deny' oder 'allow' "
"sein"

#: ../jsm/modules/mod_privacy.c:1148
msgid ""
"Privacy list is invalid: fall-through item is not allowed to have a value"
msgstr ""
"Privacy-Liste ist ungültig: Item ohne type-Attribut darf auch kein value-"
"Attribut haben"

#: ../jsm/modules/mod_privacy.c:1155
msgid "Privacy list is invalid: if type is 'jid', value has to be a valid JID"
msgstr ""
"Privacy-Liste ist ungültig: wenn der Typ 'jid' ist, muss der Value eine "
"gültige JID sein"

#: ../jsm/modules/mod_privacy.c:1161
msgid ""
"Privacy list is invalid: if type is 'group', value has to be a roster group "
"name"
msgstr ""
"Privacy-Liste ist ungültig: wenn der Typ 'group' ist, muss der Value den "
"Namen einer Rostergruppe haben"

#: ../jsm/modules/mod_privacy.c:1168
msgid ""
"Privacy list is invalid: if type is 'subscription', value has to be one of "
"'none', 'from', 'to', or 'both'"
msgstr ""
"Privacy-Liste ist ungültig: wenn der Typ 'subscription' ist, muss der Value "
"eines von 'none', 'from', 'to' oder 'both' sein"

#: ../jsm/modules/mod_privacy.c:1173
msgid ""
"Privacy list is invalid: if type is present, it has to be one of 'jid', "
"'group', or 'subscription'"
msgstr ""
"Privacy-Liste ist ungültig: wenn das type-Attribut vorhanden ist, muss es "
"einen der folgenden Werte haben: 'jid', 'group' oder 'subscription'"

#: ../jsm/modules/mod_privacy.c:1474
msgid "Blocked by recipient's privacy list"
msgstr "Gesperrt durch die Privacy-Liste des Empfängers"

#: ../jsm/modules/mod_privacy.c:1476
msgid "Blocked by your own privacy list"
msgstr "Gesperrt durch Ihre eigene Privacy-Liste"

